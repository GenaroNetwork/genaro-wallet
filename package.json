{
  "name": "genaro-wallet",
  "version": "0.2.4",
  "description": "Genaro Wallet",
  "homepage": "https://genaro.network",
  "author": {
    "name": "Geanro wallet",
    "email": "ziyi@genaro.network"
  },
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps && node angular-support",
    "ng": "ng",
    "start": "cross-env NODE_ENV=development npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "electron:tsc": "tsc main.ts --lib es5,dom,es2017,es2016,es2015",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "electron:mac:dev": "npm run build && npx electron-builder build --mac",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.3",
    "better-sqlite3": "^4.1.4",
    "bip39": "^2.5.0",
    "bitcore-lib": "^0.15.0",
    "dnode": "^1.2.2",
    "ethereumjs-wallet": "^0.6.0",
    "genaro-web3": "^0.6.1",
    "genaroshare-daemon": "^1.4.7",
    "hdkey": "^1.0.0",
    "jasmine": "^3.1.0",
    "jdenticon": "^2.1.0",
    "jswallet-manager": "^0.5.6",
    "libgenaro": "^2.0.0-alpha.11",
    "ng-zorro-antd": "^1.1.0",
    "numeral": "^2.0.6",
    "pretty-ms": "^3.1.0",
    "qrcode": "^1.2.0",
    "secp256k1": "^3.3.0",
    "storj-lib": "^8.7.2",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2",
    "web3-utils": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@angular/cli": "^6.1.0",
    "@angular/animations": "^6.1.0",
    "@angular-devkit/build-angular": "^0.7.1",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "babel-loader": "^7.1.4",
    "codelyzer": "4.2.1",
    "core-js": "2.5.6",
    "cross-env": "^5.1.3",
    "electron": "2.0.2",
    "electron-builder": "20.14.7",
    "electron-reload": "1.2.2",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-electron": "^6.0.0",
