{
  "productName": "G-BOX",
  "directories": {
    "output": "app-builds"
  },
  "files": [
    "**/*",
    "!*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!.angular-cli.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json"
  ],
  "npmRebuild": false,
  "win": {
    "icon": "dist",
    "extraFiles": [
      "msvcr110.dll"
    ],
    "rfc3161TimeStampServer": "http://sha256timestamp.ws.symantec.com/sha256/timestamp",
    "signingHashAlgorithms": ["sha256"],
    "signDlls": false,
    "target": [
      "nsis"
    ]
  },
  "mac": {
    "icon": "dist",
    "target": [
      "default"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  },
  "publish": {
    "provider": "generic",
    "url": "http://genaro-auto-update.oss-cn-shanghai.aliyuncs.com/G-BOX",
    "channel": "latest",
    "publishAutoUpdate": true
  }
}
